{
  "name": "three.js - Parcel",
  "version": "1.0.0",
  "scripts": {
    "start": "parcel ./src/project-name/index.html -p 8000 --no-source-maps",
    "build": "rm -r ./dist && parcel build ./src/project-name/index.html  --no-source-maps"
  },
  "targets": {
    "browser": {
      "publicUrl": "./"
    }
  },
  "devDependencies": {
    "parcel": "2.4.1",
    "parcel-plugin-imagemin": "2.0.0"
  }
}